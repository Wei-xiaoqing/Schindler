/* miniprogram/pages/email.wxss */
.pages{
  height: 1160rpx;
}
.tipss{
  position: relative;
  padding-left: 40rpx;
  height: 100rpx;
  line-height: 100rpx;
  background-color: rgb(253, 237, 215);
  color: rgb(255, 188, 94);
}
.tipss van-icon{
  position: absolute;
  right: 40rpx;
  top: 40rpx;
}
.main_file{
  width: 100%;
  height: 60%;
}
.file_item{
  width: 100%;
  height: 150rpx;
  line-height: 150rpx;
}
.file_title{
  position: absolute;
  left: 35rpx;
}
.contents{
  padding: 20rpx 40rpx;
  font-size: 26rpx;
  color: rgb(105, 105, 105);
}
.btn{
  width: 90%;
  margin: 0 auto;
  height: 100rpx;
  background-color: #2A5AC7;
  line-height: 100rpx;
  text-align: center;
  border-radius: 50rpx;
  color: #ffffff;
}